global:
  scrape_interval: 10s

scrape_configs:
  - job_name: 'prometheus_server_metrics'
    scrape_interval: 5s
    static_configs:
      - targets: ['localhost:9090']
  - job_name: 'polygonsdk_metrics'
    scrape_interval: 5s
    static_configs:
      - targets:
      {% for node in psdk_nodes %}
        - {{node}}:5001
      {% endfor %}